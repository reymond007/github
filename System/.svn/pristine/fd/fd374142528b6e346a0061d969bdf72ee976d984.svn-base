package cn.edu.seu.pojo;

import java.util.Set;

public class Glesson {
	private long lessonId;
	private long lessonNumber;
	private String courseName;
	private long teacherNumber;
	private Course course;
	private Teacher teacher;
	private Set courseSelector;
	public Set getCourseSelector() {
		return courseSelector;
	}
	public void setCourseSelector(Set courseSelector) {
		this.courseSelector = courseSelector;
	}
	public Teacher getTeacher() {
		return teacher;
	}
	public void setTeacher(Teacher teacher) {
		this.teacher = teacher;
	}
	public long getLessonId() {
		return lessonId;
	}
	public void setLessonId(long lessonId) {
		this.lessonId = lessonId;
	}
	public long getLessonNumber() {
		return lessonNumber;
	}
	public void setLessonNumber(long lessonNumber) {
		this.lessonNumber = lessonNumber;
	}
	public String getCourseName() {
		return courseName;
	}
	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}
	public long getTeacherNumber() {
		return teacherNumber;
	}
	public void setTeacherNumber(long teacherNumber) {
		this.teacherNumber = teacherNumber;
	}
	
	public Course getCourse() {
		return course;
	}
	public void setCourse(Course course) {
		this.course = course;
	}

}
